{
  "compilerOptions": {
    "noImplicitAny": false,
    "declaration": false,
    "removeComments": false,
    "noEmitOnError": true,
    "lib": [
      "dom",
      "es2015"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": false,
    "target": "es2017",
    "skipLibCheck": true,
    "noImplicitReturns": true,
    "noImplicitUseStrict": true,
    "typeRoots": [
      "../node_modules/@types"
    ],
    "types": [
      "node",
      "cucumber",
      "chai"
    ],
    "outDir": "../tmp/"
  },
  "include": [
    "../e2e"
  ],
  "exclude": [
    "../node_modules",
    "../src",
    "../tmp"
  ],
  "compileOnSave": false
}